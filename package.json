{
   "name": "absqra-api",
   "version": "1.0.0",
   "description": "api for absqra",
   "main": "build/server/main.js",
   "types": "lib/types",
   "scripts": {
      "test": "jest",
      "start": "npm run generate-barrels && rm -rf lib/types && tsc && ts-node $NODE_DEBUG_OPTION src/main.ts",
      "build": "tsc",
      "build:watch": "nodemon --config nodemon.json",
      "restart:db": "psql -f sqlScripts/restart.sql -v datname=\"interveydev\" -v username=\"interveyapi\" -v datnameQ=\"'interveyapi'\"",
      "generate-barrels": "barrelsby --delete -c barrelconfig.json",
      "booom": "killall node"
   },
   "repository": {
      "type": "git",
      "url": "git+ssh://git@github.com/anvlkv/absqra-server.git"
   },
   "keywords": [
      "absqra",
      "server"
   ],
   "author": "Aleksandr Novolokov",
   "license": "MIT",
   "bugs": {
      "url": "https://github.com/anvlkv/absqra-server/issues"
   },
   "jest": {
      "testEnvironment": "node"
   },
   "homepage": "https://github.com/anvlkv/absqra-server#readme",
   "dependencies": {
      "@koa/cors": "^2.2.2",
      "fast-json-patch": "^2.0.7",
      "fs": "0.0.1-security",
      "http": "0.0.0",
      "koa": "^2.6.2",
      "koa-body": "^2.6.0",
      "koa-convert": "^1.2.0",
      "koa-cookie": "^1.0.0",
      "koa-helmet": "^3.3.0",
      "koa-router": "^7.4.0",
      "koa-router2": "^1.3.1",
      "koa-send": "^4.1.3",
      "koa-session": "^5.10.0",
      "koa-session-async": "0.0.2",
      "koa-static": "^4.0.3",
      "koa-websocket": "^5.0.1",
      "node-rules": "^4.0.2",
      "nodemon": "^1.18.6",
      "pg": "^7.6.1",
      "pluralize": "^7.0.0",
      "rxjs": "^6.3.3",
      "typeorm": "^0.2.8"
   },
   "devDependencies": {
      "@types/jest": "^23.3.9",
      "@types/json-patch": "0.0.30",
      "@types/koa": "^2.0.46",
      "@types/koa-router": "^7.0.33",
      "@types/koa-send": "^4.1.1",
      "@types/koa-session": "^3.0.6",
      "@types/koa-static": "^4.0.0",
      "@types/koa-websocket": "^5.0.3",
      "@types/koa2-cors": "^2.0.1",
      "@types/node": "^8.10.37",
      "@types/pluralize": "0.0.28",
      "barrelsby": "0.0.8",
      "better-console": "^1.0.1",
      "concurrently": "^3.6.1",
      "debug": "^4.1.0",
      "jest": "^23.6.0",
      "ts-node": "^7.0.1",
      "tslint": "^5.11.0",
      "typescript": "2.7.2",
      "typescript-ioc": "^1.2.4"
   }
}
